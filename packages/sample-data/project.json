{
  "name": "sample-data",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/sample-data/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/sample-data",
        "main": "packages/sample-data/src/index.ts",
        "tsConfig": "packages/sample-data/tsconfig.lib.json",
        "assets": ["packages/sample-data/*.md", "packages/sample-data/data/*"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/sample-data/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/sample-data/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
